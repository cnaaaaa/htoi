javascript:void(function(l,s,h,ta,i,su){l=document.getElementsByTagName('a');s='';for(i=0;i<l.length;i++){h=l[i].href;if(/^ed2k:\/\//.test(h)){s+=h+'\n'}}if(s==''){alert('no ed2k l');return;}ta=document.createElement("textarea");ta.style.position='fixed';ta.style.top=0;ta.style.left=0;ta.style.width='2em';ta.style.height='2em';ta.style.padding=0;ta.style.border='none';ta.style.outline='none';ta.style.boxShadow='none';ta.style.background='transparent';ta.value=s;document.body.appendChild(ta);ta.focus();ta.select();try{su=document.execCommand('copy');var msg=su?'success':'fail';alert('copying '+msg);}catch(err){alert('Oops, unable to copy');}document.body.removeChild(ta);}())
